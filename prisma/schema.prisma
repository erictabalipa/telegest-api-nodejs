generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Teste {
  id              Int            @id @default(autoincrement())
  name            String
  TesteRelation   TesteRelation? @relation(fields: [testeRelationId], references: [id])
  testeRelationId Int?
}

model TesteRelation {
  id    Int    @id @default(autoincrement())
  phone String

  teste Teste[]
}
